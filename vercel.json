{
  "version": 2,
  "builds": [
    {
      "src": "artisan",
      "use": "@vercel/php"
    },
    {
      "src": "vite.config.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.php)$",
      "dest": "/artisan"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "buildCommand": "composer install --no-dev && php artisan config:cache && npm install && npm run build",
  "outputDirectory": "public"
}
